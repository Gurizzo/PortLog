@model IEnumerable<MVC.ViewModels.Importacion.ViewModelImportacionList>

@{
    ViewBag.Title = "Lista";
}

<h2>Lista</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FchIngreso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FchSalida)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Producto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cantidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cliente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pais)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Enviado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FchIngreso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FchSalida)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Producto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cantidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pais)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Enviado)
        </td>
        
    </tr>
}

</table>
